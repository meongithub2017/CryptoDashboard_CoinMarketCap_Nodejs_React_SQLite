{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "experimantalObjectRestSpread": true
    }
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["react", "prefer-object-spread"],
  "rules": {
    "indent": [2, 4, { "SwitchCase": 1 }],
    "prefer-object-spread/prefer-object-spread": 2
  }
}
